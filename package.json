{
  "name": "eslint-config-devjskit",
  "version": "3.0.5",
  "description": "Reasonable ESLint, and TypeScript configs for JavaScript/TypeScript devs",
  "bugs": "https://github.com/devjskit/eslint-config/issues",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "prettier": "prettier-config-devjskit",
  "author": "Billgo <hi@billgo.me>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "files": [
    "./index.js",
    "./index.mjs",
    "./index.d.ts",
    "./typescript.json"
  ],
  "imports": {
    "#/*": "./*"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "bump:beta": "npm version prerelease --preid beta",
    "publish:beta": "npm publish --tag beta"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.6.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "eslint": "^9.6.0",
    "prettier": "^3.3.2",
    "prettier-config-devjskit": "^3.0.5",
    "release-it": "^17.4.0"
  },
  "peerDependencies": {
    "eslint": "^9.0",
    "prettier": "^3.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
